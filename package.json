    "@types/jest": "29.5.2",
    "@types/lodash": "4.14.200",
    "@types/multer": "1.4.10",
    "@types/node": "20.3.1",
    "@types/nodemailer": "6.4.13",
    "@types/passport-jwt": "3.0.12",
